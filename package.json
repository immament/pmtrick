{
    "name": "pmtrick-browser-extension",
    "version": "0.0.2",
    "description": "A Planetarium Manager online football manager Browser Extension",
    "main": "index.js",
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "dev": "webpack -w --config webpack.dev.js",
        "build:dev": "webpack --config webpack.dev.js",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006",
        "sb": "yarn storybook",
        "build-storybook": "build-storybook",
        "webpack-bundle-analyzer": "webpack-bundle-analyzer",
        "create-ext": "cd dist && zip -r -FS ../output/pmtrick.zip * --exclude '*.git*'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/immament/pmtrick"
    },
    "keywords": [
